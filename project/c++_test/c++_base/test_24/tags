!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
CC	Makefile	/^CC=$(prefix)g++$/;"	m
CFLAGS	Makefile	/^CFLAGS= -Wall -O3$/;"	m
Child	test.cpp	/^class Child : public Parent {$/;"	c	file:
INCLUDE	Makefile	/^INCLUDE= -I.$/;"	m
LDFLAGS	Makefile	/^LDFLAGS=$/;"	m
NewChild	test.cpp	/^class NewChild : public Parent {$/;"	c	file:
Parent	test.cpp	/^class Parent {$/;"	c	file:
add	test.cpp	/^        int add(int a, int b) $/;"	f	class:Child
main	test.cpp	/^main(int argc, char *const argv[])$/;"	f
prefix	Makefile	/^prefix=$/;"	m
run	test.cpp	/^run(void)$/;"	f
run_1	test.cpp	/^run_1(void) $/;"	f
test	test.cpp	/^test(Parent *p)$/;"	f
~Parent	test.cpp	/^        virtual ~Parent() {}$/;"	f	class:Parent
